    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.149",
    "@types/node": "^16.11.10",
    "jest": "^24.0.0",
